{
  "name": "nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest",
    "ui": "vitest --ui",
    "preinstall": "npx only-allow pnpm",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint \"./**/*.{css,scss,html,.js,.jsx,.ts,.tsx}\"",
    "stylelint:fix": "stylelint \"./**/*.{css,scss,html,.js,.jsx,.ts,.tsx}\" --fix",
    "format": "prettier --write \"./**/*.{html,ts,js,tsx,jsx,json,md}\""
  },
  "dependencies": {
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "client-only": "^0.0.1",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "stylelint-order": "^6.0.3",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/ui": "^0.29.3",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.2.1",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.0",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "sass": "^1.59.3",
    "stylelint": "^15.2.0",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-standard": "^30.0.1",
    "tailwindcss": "^3.2.7",
    "unplugin-auto-import": "^0.15.1",
    "vitest": "^0.29.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
